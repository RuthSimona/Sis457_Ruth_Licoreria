        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Clave)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Clave)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.EsActivo)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.EsActivo)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.IdRolNavigation)
        </dt>
